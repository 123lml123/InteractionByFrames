<UserControl x:Class="bitkyFlashresUniversal.poleInfoShow.BitkyPoleControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d" Background="#7FFFFFFF" Margin="0" MinWidth="121" MinHeight="62">
    <Grid x:Name="GridMain" Background="#BF000000" Margin="5">
        <Label x:Name="LabelPoleId" Content="Label" Margin="30,0,0,0" Foreground="White" Height="25" VerticalAlignment="Top" HorizontalAlignment="Left" Width="40" FontSize="10" RenderTransformOrigin="0.833,0.5"/>
        <Label x:Name="LabelPole" Content="通道:" HorizontalAlignment="Left" Foreground="White" Height="25" VerticalAlignment="Top" Width="35" FontSize="10"/>
        <Label x:Name="LabelUnit" Content="mV" Margin="0,0,1.8,0" Foreground="White" HorizontalAlignment="Right" Width="26" Height="25" VerticalAlignment="Top" FontSize="10"/>
        <Label x:Name="LabelNum" Content="待获取" Margin="-5,15,-5,0.4" Foreground="White" HorizontalContentAlignment="Center" FontSize="20" VerticalContentAlignment="Center"/>

    </Grid>
</UserControl>
