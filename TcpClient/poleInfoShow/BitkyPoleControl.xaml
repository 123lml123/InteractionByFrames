<UserControl x:Class="bitkyFlashresUniversal.poleInfoShow.BitkyPoleControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:bitkyFlashresUniversal.poleInfoShow"
             mc:Ignorable="d" Height="61.6" Width="116.5" Background="#7FFFFFFF">
    <Grid Background="#BF000000" Margin="2">
        <Label x:Name="labelPoleId" Content="Label" Margin="30,0,0,0" Foreground="White" Height="25" VerticalAlignment="Top" HorizontalAlignment="Left" Width="40" FontSize="10" RenderTransformOrigin="0.833,0.5"/>
        <Label x:Name="labelPole" Content="电极:" HorizontalAlignment="Left" Foreground="White" Height="25" VerticalAlignment="Top" Width="35" FontSize="10"/>
        <Label x:Name="label" Content="mV" Margin="0,0,1.8,0" Foreground="White" HorizontalAlignment="Right" Width="26" Height="25" VerticalAlignment="Top" FontSize="10"/>
        <Label x:Name="labelNum" Content="待获取" Margin="0,0,0.8,4.6" Foreground="White" HorizontalContentAlignment="Center" FontSize="24" Height="38" VerticalAlignment="Bottom"/>

    </Grid>
</UserControl>
